---
import CurioCategory from '@/components/CurioCategory/index.astro'
import { PARTITIONED_CURIOS } from './_data/curios'
import Layout from './_ui/Layout3-1.astro'
---

<Layout title="奇物 差分宇宙模拟工具">
  {
    PARTITIONED_CURIOS.map((curioCate) => (
      <CurioCategory type={Number(curioCate[0].Type)} curios={curioCate} />
    ))
  }
</Layout>
